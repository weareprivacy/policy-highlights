{
  "name": "@weareprivacy/policy-highlights",
  "version": "1.8.0",
  "description": "Automatically highlight keywords on your privacy policy, cookie policy, disclosure policy, disclaimer policy, and terms of service so the user can quickly find and understand important sections.",
  "main": "dist/bundle.js",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --mode development --open",
    "dev": "webpack --mode development",
    "build": "webpack --mode production --progress",
    "docs": "./node_modules/.bin/esdoc"
  },
  "keywords": [
    "privacy policy",
    "cookie policy",
    "disclosure policy",
    "disclaimer policy",
    "terms of service",
    "highlighter",
    "policies",
    "privacy",
    "tos",
    "highlight",
    "policy highlights",
    "patriot act",
    "gdpr",
    "terms",
    "disclosure",
    "disclaimer",
    "cookie",
    "conduct",
    "legal",
    "sign",
    "contract",
    "confirm",
    "agreement"
  ],
  "author": {
    "name": "Pawel Glowacki",
    "email": "team@weareprivacy.com",
    "web": "https://weareprivacy.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weareprivacy/policy-highlights"
  },
  "bugs": {
    "url": "https://github.com/weareprivacy/policy-highlights/issues"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.6.3",
    "acorn": "^6.3.0",
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^6.0.0",
    "img-loader": "^3.0.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "tar": "^4.4.13",
    "url-loader": "^1.1.1",
    "webpack": "^4.41.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.8.2"
  },
  "resolutions": {
    "webpack/acorn": "6.1.1",
    "tar": ">=4.4.2",
    "fstream": ">=1.0.12"
  }
}
